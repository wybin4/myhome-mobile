<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Myhome" parent="Theme.MaterialComponents.DayNight">
        <item name="colorAccent">@color/primary</item>
    </style>

    <style name="Input" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxCornerRadiusTopEnd">@dimen/input_border</item>
        <item name="boxCornerRadiusTopStart">@dimen/input_border</item>
        <item name="boxCornerRadiusBottomStart">@dimen/input_border</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/input_border</item>
        <item name="errorEnabled">true</item>
        <item name="errorIconTint">@color/white</item>
        <item name="android:layout_marginBottom">@dimen/input_margin_bottom</item>
    </style>

    <style name="Dropdown" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxCornerRadiusTopEnd">@dimen/input_border</item>
        <item name="boxCornerRadiusTopStart">@dimen/input_border</item>
        <item name="boxCornerRadiusBottomStart">@dimen/input_border</item>
        <item name="boxCornerRadiusBottomEnd">@dimen/input_border</item>
        <item name="errorEnabled">true</item>
        <item name="android:layout_marginBottom">@dimen/input_margin_bottom</item>
    </style>

    <style name="ActionButton">
        <item name="android:layout_height">58dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14dp</item>
        <item name="textAllCaps">false</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:backgroundTint">@color/button_states_normal</item>
    </style>

    <style name="SwapButton">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">15dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:paddingTop">9dp</item>
        <item name="android:paddingBottom">9dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingLeft">20dp</item>
    </style>

    <style name="FloatingActionButton">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="elevation">0dp</item>
        <item name="borderWidth">0dp</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:src">@drawable/add</item>
        <item name="tint">@color/white</item>
        <item name="maxImageSize">14dp</item>
        <item name="android:layout_marginBottom">90dp</item>
    </style>

    <style name="FloatingActionButtonSubstrate">
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="ActionButtonWithDrawable" parent="ActionButton">
        <item name="android:drawableTint">@color/white</item>
        <item name="cornerRadius">100dp</item>
    </style>

    <style name="ItemLoading">
        <item name="android:layout_width">match_parent</item>
        <item name="android:backgroundTint">@color/primary_loading</item>
        <item name="android:background">@drawable/round_corner_button</item>
    </style>

    <style name="DescriptionText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/description_normal</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="AttentionCard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:background">@drawable/round_corner_card_normal</item>
        <item name="android:paddingLeft">23dp</item>
        <item name="android:paddingRight">23dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
    </style>

    <style name="AttentionCardText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="CircleBlackPrimary">
        <item name="android:background">@drawable/circle_normal</item>
    </style>

    <style name="CircleBlackWhite">
        <item name="android:background">@drawable/circle_normal</item>
    </style>

    <style name="ImageBlackWhite">
        <item name="tint">@color/white</item>
    </style>

    <style name="CircleButtonLayout">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="CircleButton">
        <item name="android:background">@drawable/circle_button_normal</item>
        <item name="android:layout_width">43dp</item>
        <item name="android:layout_height">43dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="CircleButtonImage">
        <item name="tint">@color/primary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/transparent</item>
    </style>

    <style name="CircleButtonText">
        <item name="android:textColor">@color/primary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Text">
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="WhiteBlackText">
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="BlackWhiteButtonLayout">
        <item name="android:background">@drawable/circle_button_normal</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="cornerRadius">15dp</item>
    </style>

    <style name="ChatBlackWhite">
        <item name="android:background">@drawable/round_corner_button</item>
        <item name="android:backgroundTint">@color/black</item>
    </style>

    <style name="ChatPrimaryLight">
        <item name="android:background">@color/primary_light_normal_without_alpha</item>
        <item name="android:backgroundTint">@color/primary_light_normal_without_alpha</item>
    </style>

    <style name="ChatPrimaryLightTail">
        <item name="tint">@color/primary_light_normal_without_alpha</item>
    </style>

    <style name="ImageLikeText">
        <item name="tint">@color/black</item>
    </style>

    <style name="DigitButton">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">32sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">112sp</item>
        <item name="android:layout_height">79sp</item>
        <item name="backgroundTint">@color/transparent</item>
        <item name="android:background">@null</item>
        <item name="android:elevation">0dp</item>
        <item name="android:stateListAnimator">@null</item>
    </style>

    <style name="DigitButtonDelete">
        <item name="tint">@color/black</item>
    </style>

    <style name="Card">
        <item name="android:background">@drawable/round_corner_card_normal</item>
    </style>

    <style name="TextInput">
        <item name="android:background">@drawable/round_corner_input_normal</item>
    </style>

    <style name="BackgroundWithElevation">
        <item name="android:background">@color/white</item>
    </style>

    <style name="ChargeBill">
        <item name="srcCompat">@drawable/charge_bill_normal</item>
    </style>

    <style name="Dialog">
        <item name="android:background">@drawable/dialog_normal</item>
    </style>

    <style name="PickCardLayout">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">175dp</item>
        <item name="android:background">@drawable/round_corner_button</item>
        <item name="android:backgroundTint">@color/primary_light</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="IconBlackWhiteLayout">
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/circle_normal</item>
        <item name="android:backgroundTint">@color/primary</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>

    <style name="IconWhite">
        <item name="android:layout_width">23dp</item>
        <item name="android:layout_height">23dp</item>
        <item name="tint">@color/white</item>
    </style>

    <style name="IconBlackPrimary">
        <item name="tint">@color/black</item>
    </style>

    <style name="IconBlackWhite">
        <item name="tint">@color/black</item>
    </style>

    <style name="PickCardText" parent="Text">
        <item name="android:layout_width">112dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="BottomSheetDialog">
        <item name="android:background">@drawable/bottom_dialog_normal</item>
    </style>

    <style name="ErrorListLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginTop">-130dp</item>
    </style>

    <style name="EmptyListLayout">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="SheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:colorBackground">@android:color/transparent</item>
    </style>

</resources>